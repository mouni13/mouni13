#comparison operators
a=int(input('enter a:'))
b=int(input('enter b:'))
if(a>b)
Print(true)
Else
Print(false)
if(a<b)
Print(true)
Else
Print(false)
if(a>=b)
Print(true)
Else
Print(false)
if(a<=b)
Print(true)
Else
Print(false)
if(a==b)
Print(true)
Else
Print(false)
if(a!=b)
Print(true)
Else
Print(false)
